{
    "name": "hyperf-aliyun-log/aliyun-sls",
    "description": "AliYun SLS Log For Hyperf",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "aliyun",
        "sls"
    ],
    "authors": [
        {
            "name": "xiaoyin199",
            "email": "xiaoyin199@126.com"
        }
    ],
    "require": {
        "hyperf/guzzle": "^2.0.0",
        "hyperf/contract": "^2.0.0",
        "psr/container": "^1.0",
        "php": ">=7.2"
    },
    "require-dev": {
        "hyperf/config": "^2.0.0",
        "hyperf/event": "^2.0.0",
        "hyperf/framework": "^2.0.0",
        "hyperf/process": "^2.0.0",
        "malukenho/docheader": "^0.1.6",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0.0",
        "friendsofphp/php-cs-fixer": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Ym\\AliyunSls\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YmTest\\AliyunSls\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "Ym\\AliyunSls\\ConfigProvider"
        }
    }
}
